<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="data.models.Country" table="country">
        <id name="countryID" column="countryID">
            <generator class="increment"/>
        </id>

        <property name="name" column="name" not-null="true" type="string" unique="true"/>
        <property name="code" column="code" not-null="true" type="string" unique="true"/>
   
    </class>
</hibernate-mapping>
